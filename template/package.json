{
    "name": "next-app-template",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "20"
    },
    "type": "module",
    "scripts": {
        "dev:vercel": "vercel env pull && vercel dev",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
        "lokse": "lokse open",
        "localize": "lokse update",
        "type-check": "tsc",
        "circular-modules": "madge --circular src",
        "test": "jest",
        "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|html|css|md)\" --write",
        "ci-test": "jest --ci",
        "ci-lint": "yarn lint",
        "ci-circular-modules": "yarn circular-modules",
        "generate-icons": "npx @svgr/cli src/modules/ui/components/Icons/Icons/assets",
        "audit:high": "yarn audit --level high --groups dependencies",
        "version": "yarn changelog && code CHANGELOG.md --wait && git add CHANGELOG.md .env",
        "changelog": "git fetch origin && npx gitmoji-changelog",
        "storybook:dev": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.1",
        "@sentry/nextjs": "^7.52.1",
        "@t3-oss/env-core": "^0.6.1",
        "@tanstack/react-query": "^4.35.0",
        "lodash": "^4.17.21",
        "loglevel": "^1.8.1",
        "next": "13.4.19",
        "normalize.css": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.46.1",
        "react-intl": "^6.4.6",
        "reset.css": "^2.0.2",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@ackee/browserslist-config": "^3.0.0",
        "@ackee/eslint-config": "^4.1.1",
        "@ackee/prettier-config": "^1.1.0",
        "@babel/core": "^7.22.17",
        "@lokse/plugin-prettier": "^2.3.1",
        "@storybook/addon-actions": "^7.4.1",
        "@storybook/addon-essentials": "^7.4.1",
        "@storybook/addon-interactions": "^7.4.1",
        "@storybook/jest": "^0.2.2",
        "@storybook/nextjs": "^7.4.1",
        "@storybook/react": "^7.4.1",
        "@storybook/testing-library": "^0.2.0",
        "@tanstack/eslint-plugin-query": "^4.34.1",
        "@tanstack/react-query-devtools": "^4.35.0",
        "@testing-library/jest-dom": "^6.1.3",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.4",
        "@types/lodash": "^4.14.194",
        "@types/node": "18.0.6",
        "@types/react": "^18.2.12",
        "@types/react-dom": "18.2.4",
        "@typescript-eslint/eslint-plugin": "5.59.6",
        "@typescript-eslint/parser": "5.59.6",
        "eslint": "8.40.0",
        "eslint-config-next": "^13.4.3",
        "eslint-config-prettier": "8.8.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-compat": "^4.1.4",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-storybook": "^0.6.13",
        "fork-ts-checker-webpack-plugin": "^6.5.0",
        "husky": "^8.0.0",
        "jest": "^29.6.4",
        "lint-staged": "14.0.1",
        "lokse": "^2.3.1",
        "madge": "^6.1.0",
        "next-router-mock": "^0.9.9",
        "prettier": "^3.0.3",
        "storybook": "^7.4.1",
        "typescript": "5.0.4"
    },
    "browserslist": [
        "extends @ackee/browserslist-config"
    ],
    "lint-staged": {
        "*.{ts,js,tsx,json,html,css,md}": [
            "yarn format && yarn lint:fix"
        ],
        "**/*.{png,jpeg,jpg,gif,svg,webp}": [
            "imagemin-lint-staged"
        ]
    },
    "readme": "ERROR: No README data found!",
    "_id": "next-app-template@0.1.0"
}