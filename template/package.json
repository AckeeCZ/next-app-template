{
    "name": "next-ackee-app-template",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "20"
    },
    "scripts": {
        "dev:vercel": "vercel env pull && vercel dev",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "localize": "lokse update",
        "type-check": "tsc",
        "circular-modules": "madge --circular src",
        "test": "jest",
        "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|html|css|md)\" --write",
        "ci-test": "jest --ci",
        "ci-lint": "yarn lint",
        "ci-circular-modules": "yarn circular-modules",
        "storybook:start": "start-storybook -p 6006",
        "storybook:build": "build-storybook",
        "storybook:snapshot": "npx chromatic --project-token=<passwd_token>",
        "generate-icons": "npx @svgr/cli src/modules/ui/components/Icons/Icons/assets",
        "audit:high": "yarn audit --level high --groups dependencies",
        "prepare": "husky install",
        "version": "yarn changelog && code CHANGELOG.md --wait && git add CHANGELOG.md .env",
        "changelog": "git fetch origin && gitmoji-changelog"
    },
    "dependencies": {
        "@sentry/nextjs": "7.52.1",
        "@stitches/react": "1.2.8",
        "@tanstack/react-query": "4.29.7",
        "@tanstack/react-query-devtools": "4.29.7",
        "lodash": "4.17.21",
        "next": "13.4.3",
        "normalize.css": "8.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-intl": "6.4.2",
        "reset.css": "2.0.2"
    },
    "devDependencies": {
        "@ackee/browserslist-config": "3.0.0",
        "@ackee/eslint-config": "4.1.1",
        "@lokse/plugin-prettier": "2.3.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/lodash": "4.14.194",
        "@types/node": "20.2.1",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@typescript-eslint/eslint-plugin": "5.59.6",
        "@typescript-eslint/parser": "5.59.6",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.3",
        "eslint-config-prettier": "8.8.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-compat": "4.1.4",
        "eslint-plugin-prettier": "4.2.1",
        "fork-ts-checker-webpack-plugin": "8.0.0",
        "gitmoji-changelog": "^2.3.0",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "lint-staged": "13.2.2",
        "lokse": "2.3.1",
        "madge": "6.0.0",
        "next-router-mock": "0.9.3",
        "prettier": "2.8.8",
        "prettier-config-ackee": "0.0.16",
        "typescript": "5.0.4"
    },
    "optionalDependencies": {
        "@babel/core": "7.20.12",
        "@storybook/addon-actions": "6.5.16",
        "@storybook/addon-essentials": "6.5.16",
        "@storybook/addon-interactions": "6.5.16",
        "@storybook/addon-links": "6.5.16",
        "@storybook/builder-webpack5": "6.5.16",
        "@storybook/manager-webpack5": "6.5.16",
        "@storybook/react": "6.5.16",
        "@storybook/testing-library": "0.1.0",
        "babel-loader": "8.3.0",
        "storybook-addon-next": "1.8.0",
        "tsconfig-paths-webpack-plugin": "4.0.0"
    },
    "browserslist": [
        "extends @ackee/browserslist-config"
    ],
    "lint-staged": {
        "*.{ts,js,tsx,json,html,css,md}": [
            "yarn format"
        ],
        "**/*.{png,jpeg,jpg,gif,svg,webp}": [
            "imagemin-lint-staged"
        ]
    },
    "readme": "ERROR: No README data found!",
    "_id": "next-app-template@0.1.0"
}
