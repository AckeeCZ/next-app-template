{
    "name": "web",
    "version": "0.0.1",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint-lint --config=eslint.config.mjs './src/**/*.{ts,tsx}'",
        "lint:fix": "yarn lint --fix",
        "lokse": "lokse open",
        "localize": "lokse update",
        "test": "vitest dev",
        "test:ci": "vitest run",
        "version": "yarn changelog && code CHANGELOG.md --wait && git add CHANGELOG.md",
        "changelog": "git fetch origin && gitmoji-changelog",
        "format": "prettier-format",
        "cir-dep": "find-circular-deps"
    },
    "dependencies": {
        "@hookform/error-message": "^2.0.1",
        "@hookform/resolvers": "^3.3.4",
        "@sentry/nextjs": "^7.109.0",
        "@tanstack/react-query": "^5.29.0",
        "@tanstack/react-query-devtools": "^5.29.0",
        "@workspace/api": "workspace:*",
        "@workspace/common": "workspace:*",
        "@workspace/env": "workspace:*",
        "@workspace/errors": "workspace:*",
        "@workspace/localization": "workspace:*",
        "@workspace/logger": "workspace:*",
        "next": "^14.1.4",
        "normalize.css": "^8.0.1",
        "radash": "^12.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.2",
        "react-intl": "^6.6.5",
        "reset.css": "^2.0.2",
        "string-ts": "^2.1.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@tooling/eslint": "workspace:*",
        "@tooling/lokse": "workspace:*",
        "@tooling/prettier": "workspace:*",
        "@tooling/skott": "workspace:*",
        "@tooling/typescript": "workspace:*",
        "@types/react": "^18.2.75",
        "@types/react-dom": "18.2.24",
        "browserslist-config-ackee": "workspace:*",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "gitmoji-changelog": "^2.3.0",
        "type-fest": "^4.15.0",
        "typescript": "^5.4.4",
        "vitest": "^1.4.0"
    },
    "prettier": "@tooling/prettier/config",
    "browserslist": [
        "extends browserslist-config-ackee"
    ]
}
