{
    "name": "web",
    "version": "0.0.1",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint-lint --config=eslint.config.mjs './src/**/*.{ts,tsx}'",
        "lint:fix": "yarn lint --fix",
        "test": "vitest dev",
        "test:ci": "vitest run",
        "release": "node ./scripts/release.mjs && git push origin --tags",
        "format": "prettier-format",
        "cir-dep": "check-cir-deps ./src"
    },
    "dependencies": {
        "@hookform/error-message": "2.0.1",
        "@hookform/resolvers": "3.9.0",
        "@sentry/nextjs": "8.34.0",
        "@tanstack/react-query": "5.59.9",
        "@tanstack/react-query-devtools": "5.59.9",
        "@workspace/api": "workspace:*",
        "@workspace/common": "workspace:*",
        "@workspace/env": "workspace:*",
        "@workspace/errors": "workspace:*",
        "@workspace/localization": "workspace:*",
        "@workspace/logger": "workspace:*",
        "next": "14.2.15",
        "normalize.css": "8.0.1",
        "radash": "12.1.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "7.53.0",
        "reset.css": "2.0.2",
        "string-ts": "2.2.0",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@tooling/eslint": "workspace:*",
        "@tooling/madge": "workspace:*",
        "@tooling/prettier": "workspace:*",
        "@tooling/typescript": "workspace:*",
        "@types/react": "18.3.11",
        "@types/react-dom": "18.3.1",
        "browserslist-config-ackee": "workspace:*",
        "dotenv": "16.4.5",
        "fork-ts-checker-webpack-plugin": "9.0.2",
        "gitmoji-changelog": "2.3.0",
        "type-fest": "4.26.1",
        "typescript": "5.6.3",
        "vitest": "2.1.2"
    },
    "prettier": "@tooling/prettier/config",
    "browserslist": [
        "extends browserslist-config-ackee"
    ]
}
